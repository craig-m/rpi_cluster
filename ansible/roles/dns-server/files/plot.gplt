set term png truecolor size 800,600
set output "/root/data.png"
set xlabel "Domains"
set ylabel "Queries"
set title "DNS Usage"
set grid
set xtics rotate by -45 axis out offset 0,1.5
set boxwidth 1 relative
set style fill transparent solid 0.5 border rgb"black"
plot "/root/dns-data" u 1:xtic(2) w boxes lc rgb"blue"